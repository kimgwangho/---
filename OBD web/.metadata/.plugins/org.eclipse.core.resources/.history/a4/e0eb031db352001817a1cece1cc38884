package com.pro.dto;

public class ProductVO {
	private String prdct_code;
	private String data;
	private String sms;
	private String tel;
	private String price;
	private String remain_sch_cd;
	private String desc;
	private String rcv_num;
	private String company;
	private String duration;
	private String prdct_name;
	private String remain_sch_cd_money;
	
	
	public String getRcv_num() {
		return rcv_num;
	}
	public void setRcv_num(String rcv_num) {
		this.rcv_num = rcv_num;
	}
	public String getCompany() {
		return company;
	}
	public void setCompany(String company) {
		this.company = company;
	}
	public String getDuration() {
		return duration;
	}
	public void setDuration(String duration) {
		this.duration = duration;
	}
	public String getRdct_name() {
		return rdct_name;
	}
	public void setRdct_name(String rdct_name) {
		this.rdct_name = rdct_name;
	}
	public String getRemain_sch_cd_money() {
		return remain_sch_cd_money;
	}
	public void setRemain_sch_cd_money(String remain_sch_cd_money) {
		this.remain_sch_cd_money = remain_sch_cd_money;
	}
	public String getPrdct_code() {
		return prdct_code;
	}
	public void setPrdct_code(String prdct_code) {
		this.prdct_code = prdct_code;
	}
	public String getData() {
		return data;
	}
	public void setData(String data) {
		this.data = data;
	}
	public String getSms() {
		return sms;
	}
	public void setSms(String sms) {
		this.sms = sms;
	}
	public String getTel() {
		return tel;
	}
	public void setTel(String tel) {
		this.tel = tel;
	}
	public String getPrice() {
		return price;
	}
	public void setPrice(String price) {
		this.price = price;
	}
	public String getRemain_sch_cd() {
		return remain_sch_cd;
	}
	public void setRemain_sch_cd(String remain_sch_cd) {
		this.remain_sch_cd = remain_sch_cd;
	}
	public String getDesc() {
		return desc;
	}
	public void setDesc(String desc) {
		this.desc = desc;
	}
}
